  "private": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0"
  },
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --hot --history-api-fallback --config build/webpack.base.config.js",
    "build:dev": "npm run clear && webpack --config build/webpack.base.config.js --progress --display-error-details",
    "build:prod": "npm run clear && webpack --config build/webpack.prod.config.js --progress --display-error-details",
    "ngrok": "ngrok http",
    "clear": "rimraf dist/",
    "check:deps": "npm-check-updates"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "font-awesome": "^4.7.0",
    "js-cookie": "^2.1.4",
